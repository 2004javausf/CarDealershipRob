package com.revature.beans;

public class Car {
	
	private int vinNumber;
	private String make;
	private String model;
	private String color;
	private int year;
	private double price;
	private String sold = false;
	
	public Car() {
		super();
		
	}

	public int getVinNumber() {
		return vinNumber;
	}

	public void setVinNumber(int carId) {
		this.vinNumber = carId;
	}

	public String getMake() {
		return make;
	}

	public void setMake(String make) {
		this.make = make;
	}

	public String getModel() {
		return model;
	}

	public void setModel(String model) {
		this.model = model;
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}

	public double getPrice() {
		return price;
	}

	public void setPrice(double price) {
		this.price = price;
	}
	
	public String isSold() {
		return sold;
	}

	public void setSold(String sold) {
		this.sold = sold;
	}

	public Car(int vinNumber, String make, String model, String color, int year, double price, String sold) {
		super();
		this.vinNumber = vinNumber;
		this.make = make;
		this.model = model;
		this.color = color;
		this.year = year;
		this.price = price;
		this.sold = sold;
	}

	@Override
	public String toString() {
		return "Car [vinNumber=" + vinNumber + ", make=" + make + ", model=" + model + ", color=" + color + ", year=" + year
				+ ", price=" + price + ", sold=" + sold + "]";
	}
}
